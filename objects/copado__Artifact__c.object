    <validationRules>
        <fullName>copado__ArtifactNameCannotChange</fullName>
        <active>true</active>
        <errorConditionFormula>PRIORVALUE(Name) != Name</errorConditionFormula>
        <errorMessage>Artifact names are in use for DX folder structure, therefore, artifact names are not editable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>copado__Artifact_Repo_Control_For_Package_Type</fullName>
        <active>true</active>
        <description>Artifact Reposiroty null control depending of Package Type</description>
        <errorConditionFormula>IF(
OR(
AND(ISBLANK(copado__Artifact_Repository__c), 
(ISPICKVAL(copado__Package_Type__c, &apos;Unlocked&apos;) || ISPICKVAL(copado__Package_Type__c, &apos;Second-Generation Package&apos;))),
(NOT(ISBLANK(copado__Artifact_Repository__c)) &amp;&amp; ISPICKVAL(copado__Package_Type__c, &apos;Unmanaged&apos;))), true, false
)</errorConditionFormula>
        <errorDisplayField>copado__Artifact_Repository__c</errorDisplayField>
        <errorMessage>Repository relation for packages is required for Unlocked and Second Generation Packages and should be empty for Unmanaged Packages</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>copado__Name_should_not_contain_empty_space</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(Name,&apos; &apos;)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Artifact name cannot contain white spaces</errorMessage>
    </validationRules>
